{
  "name": "pat-bot",
  "version": "0.2.2",
  "description": "A silly Discord bot named after my cat.",
  "license": "MIT",
  "repository": "https://github.com/ChadLefort/pat-bot.git",
  "author": "Chad Lefort",
  "keywords": [
    "discord",
    "bot"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "deploy": "tsc --pretty && webpack --config webpack.production.config.js && npm run assets && npm run views && node lib/pm2.js",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "assets": "ncp src/web/assets lib/web/public/assets",
    "views": "mkdirp lib/web/server/views && ncp src/web/server/views lib/web/server/views",
    "build": "npm run clean && npm run lint && webpack && npm run assets && npm run views && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**/*-spec.ts",
    "develop": "nf start -j Procfile-dev",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "preversion": "npm run test",
    "version": "",
    "postversion": "git push && git push --tags"
  },
