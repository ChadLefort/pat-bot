{
    "version": "0.2.0",
    "configurations": [
        {
            "args": [],
            "cwd": "${workspaceRoot}",
            "env": {
                "NODE_ENV": "development"
            },
            "name": "DEBUG",
            "outDir": "${workspaceRoot}/lib",
            "program": "${workspaceRoot}/src/index.ts",
            "request": "launch",
            "runtimeArgs": [
                "--nolazy"
            ],
            "runtimeExecutable": null,
            "sourceMaps": true,
            "stopOnEntry": false,
            "type": "node"
        },
        {
            "name": "ATTACH",
            "type": "node",
            "request": "attach",
            "port": 5858,
            "restart": false,
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/lib",
            "localRoot": "${workspaceRoot}/src/"
        }
    ]
}